EggsX1:AddToggle("Auto Buy 500 Eggs", function(arg) --the (arg) is if the checkbox is toggled or not
    if arg then
        _G.a = true 
        while wait(0) and _G.a do
local args = {
    [1] = "EggOne"
}

game:GetService("ReplicatedStorage").Remotes.CanBuyEgg:InvokeServer(unpack(args))

        end
    else
        _G.a = false
    end
end)

