local ToInteract = game:GetService("Workspace")["SocialPark"].NPC.MoveRelearner

local ToInteractTable = nil
local Client = require(game.Players.LocalPlayer.Packer.Client)
for i,v in pairs(Client.CurrentNPCs) do
    if i == ToInteract then
        ToInteractTable = v
    end
end

ToInteractTable.Event()
